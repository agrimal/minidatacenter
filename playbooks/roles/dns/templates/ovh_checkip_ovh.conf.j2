; Configuration obtained from https://eu.api.ovh.com/createApp/

[default]
; general configuration: default endpoint
endpoint=ovh-eu

[ovh-eu]
; configuration specific to 'ovh-eu' endpoint
application_key={{ ovh_checkip_application_key }}
application_secret={{ ovh_checkip_application_secret }}

; uncomment following line when writing a script application
; with a single consumer key.
consumer_key={{ ovh_checkip_consumer_key }}
